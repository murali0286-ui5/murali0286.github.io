<mvc:View xmlns:mvc="sap.ui.core.mvc"
		  xmlns:layout="sap.ui.layout"
		  xmlns:forms="sap.ui.layout.form"
		  xmlns="sap.m">
	<forms:SimpleForm
			maxContainerCols="1"
			layout="ResponsiveGridLayout"
			width="100%">
		<layout:VerticalLayout
			content="{
				path : 'Resume>/Summary'
			}">
			<Label text="{Resume>Point}" design="Bold" class="sapUiTinyMarginBottom"/>
		</layout:VerticalLayout>
	</forms:SimpleForm>
</mvc:View>
