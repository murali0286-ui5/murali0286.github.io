<mvc:View xmlns:mvc="sap.ui.core.mvc"
		  xmlns:layout="sap.ui.layout"
		  xmlns:core="sap.ui.core"
		  xmlns:forms="sap.ui.layout.form"
		  xmlns="sap.m">
	<forms:SimpleForm labelSpanL="4" labelSpanM="4"
					  labelSpanS="4" emptySpanL="0" emptySpanM="0" emptySpanS="0"
					  maxContainerCols="2"
					  layout="ResponsiveLayout"
					  width="100%">
		<core:Title text="Phone Numbers"></core:Title>
		<Label text="Primary"/>
		<Text text="{Resume>/Personal/PhoneNumber/Primary}"/>
		<Label text="Alternate"/>
		<Text text="{Resume>/Personal/PhoneNumber/Secondary}"/>
	</forms:SimpleForm>
</mvc:View>
